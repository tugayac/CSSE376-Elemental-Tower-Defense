		return speed;
	}

	public int getCost() {
		// TODO Auto-generated method stub
		return cost;
	}

	public int getLevel() {
		// TODO Auto-generated method stub
		return level;
	}

	public void upgrade() {
		// TODO Auto-generated method stub
		
	}

}
