import java.awt.geom.Point2D;


public class Tower {

	int id;
	Object element;
	Point2D location;
	String name;
	
	
	public Object getElement() {
		// TODO Auto-generated method stub
		return element;
	}

	public Point2D getLocation() {
		// TODO Auto-generated method stub
		return location;
	}

	public int getID() {
		// TODO Auto-generated method stub
		return id;
	}

	public String getName() {
		// TODO Auto-generated method stub
		return name;
	}

	public boolean isAvailable() {
		// TODO Auto-generated method stub
		return false;
	}

}
