	private void initBottomLeftPanel() {
		this.jPanel4.setBorder(BorderFactory
				.createLineBorder(new Color(0, 0, 0)));

		this.jLabel29.setFont(new Font("Tahoma", 0, 18)); // NOI18N
		this.jLabel29.setHorizontalAlignment(javax.swing.SwingConstants.CENTER);
		this.jLabel29.setText("Available Towers");

		this.jLabel30.setText("Cost:");

		this.jLabel31.setText("jLabel31");

		this.jLabel32.setText("Cost:");

		this.jLabel33.setText("jLabel33");

		this.jLabel35.setText("Cost:");

		this.jLabel36.setText("jLabel36");

		this.jLabel37.setText("Cost:");

		this.jLabel38.setText("jLabel38");

		this.jLabel39.setText("Cost:");

		this.jLabel40.setText("jLabel40");

		ImageIcon fire = new ImageIcon(getClass().getResource(
				"/resources/images/towers/fire_unclicked.png"));
		Image temp = fire.getImage();
		Image newTemp = temp.getScaledInstance(100, 100, Image.SCALE_SMOOTH);
		this.fireButtonLabel.setIcon(new ImageIcon(newTemp));

		ImageIcon water = new ImageIcon(getClass().getResource(
				"/resources/images/towers/water_unclicked.png"));
		temp = water.getImage();
		newTemp = temp.getScaledInstance(100, 100, Image.SCALE_SMOOTH);
		this.jLabel49.setIcon(new ImageIcon(newTemp));

		ImageIcon air = new ImageIcon(getClass().getResource(
				"/resources/images/towers/air_unclicked.png"));
		temp = air.getImage();
		newTemp = temp.getScaledInstance(100, 100, Image.SCALE_SMOOTH);
		this.jLabel50.setIcon(new ImageIcon(newTemp));

		ImageIcon light = new ImageIcon(getClass().getResource(
				"/resources/images/towers/light_unclicked.png"));
		temp = light.getImage();
		newTemp = temp.getScaledInstance(100, 100, Image.SCALE_SMOOTH);
		this.jLabel51.setIcon(new ImageIcon(newTemp));

		ImageIcon earth = new ImageIcon(getClass().getResource(
				"/resources/images/towers/earth_unclicked.png"));
		temp = earth.getImage();
		newTemp = temp.getScaledInstance(100, 100, Image.SCALE_SMOOTH);
		this.jLabel52.setIcon(new ImageIcon(newTemp));
	}

